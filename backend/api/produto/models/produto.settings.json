{
  "kind": "collectionType",
  "collectionName": "produtos",
  "info": {
    "name": "Produto"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nome": {
      "type": "string"
    },
    "foto": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "Slug": {
      "type": "uid",
      "targetField": "nome"
    },
    "descricao": {
      "type": "richtext"
    },
    "meta_description": {
      "type": "text"
    },
    "meta_title": {
      "type": "text"
    },
    "preco": {
      "type": "decimal"
    }
  }
}
